/*城市切换*/

.city-selector {
  display: inline-block;
}

.city-location {
  text-align: center;
  background-color: white;
}

.city-picker {
}

.city-location .city_change {
  /*width: 160rpx;
  height: 60rpx;*/
  display: inline-block;
  border-radius: 17px;
  font-size: 0.8rem;
  background-color: #0072ca;
  color: white;
  text-align: center;
  vertical-align: middle;
  padding: 4px 37px 3px 17px;
  position: relative;
}

.city-location .city_change::after {
  content: " ";
  display: inline-block;
  height: 6px;
  width: 6px;
  border-width: 0 2px 2px 0;
  border-color: #fff;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  top: -2px;
  position: absolute;
  top: 50%;
  left: auto;
  right: 15px;
  margin-top: -5px;
}

.left-line, .right-line {
  border-top: 1px solid #b4b4b4;
  border-color: #ccc;
  display: inline-block;
  width: 32px;
  position: relative;
  vertical-align: middle;
}

.need-info {
  height: 30px;
  overflow: hidden;
}

.need-info .info {
  display: inline-block;
  margin: 0 12px;
  font-size: 0.67rem;
  color: #a2a2a2;
}

.need-info .info-item {
  height: 30px;
  line-height: 30px;
}

/*最新信息*/

.between-gap-18 {
  margin-top: 9px;
}

.lastest-info {
  padding-top: 10px;
  background-color: white;
}

.lastest-info-nav {
  position: relative;
  margin-bottom: 10px;
  background: #fff;
}

.info-tag {
  height: 16px;
  width: 4px;
  display: inline-block;
  background-color: #ff3a55;
}

.lastest-info-title {
  display: inline-block;
  margin-left: 15px;
  color: #424242;
}

.see-more {
  display: inline-block;
  position: absolute;
  right: 37px;
  font-size: 0.8rem;
  color: #666;
}

.see-more::after {
  content: " ";
  display: inline-block;
  height: 6px;
  width: 6px;
  border-width: 2px 2px 0 0;
  border-color: #666;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  top: -2px;
  position: absolute;
  top: 50%;
  left: auto;
  right: -12px;
  margin-top: -4px;
}

.lastest-info-list {
  padding: 0 20px;
}

.lastest-info-list .siting-text-block .siting-info-text-title {
